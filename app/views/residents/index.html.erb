<div class="container">
  <div class="header">
    <h3>
      Residents
    </h3>
  </div>
</div>
<div class="row">
  <div class="col-md-6">
    <table class="table table-striped table-hover">
      <tr class="success">
        <th>First name</th>
        <th>Last name</th>
        <th>Resident mb</th>

      </tr>

      <% @residents.each do |resident| %>
      <tr>
        <td><%= resident.first_name %></td>
        <td><%= resident.last_name %></td>
        <td><%= resident.resident_mb_id %></td>

      </tr>
      <% end %>
    </table>
  </div>
</div>
