<div class="container">
  <div class="header">
    <h3>
      My RSVPs
    </h3>
  </div>
</div>

<div class= "container">
  <div class="row">
    <div class="col-md-6">
      <table class="table table-striped table-hover">
        <tr>
          <th>Session</th>
          <th>Resident</th>
          <th>Confirmed</th>

        </tr>

        <% @rsvps.each do |rsvp| %>
        <tr>

          <td><%= rsvp.session.session_name %></td>
          <td><%= rsvp.resident.full_name %></td>
          <td>
            <% if %>
            <% rsvp.confirmed == true %>
            Yes
            <% else  %>
            <% end %>
          </td>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
